{
  "name": "nestjs-backend-starter",
  "version": "0.0.2",
  "description": "A small nestjs backend with integration of mysql / sqlite",
  "author": {
    "email": "blueskyfish@blueskyfish.de",
    "name": "Blueskyfish",
    "url": "https://blueskyfish.de"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/blueskyfish/nestjs-backend-starter.git"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "express",
    "starter",
    "javascript",
    "docker",
    "mysql",
    "sqlite",
    "backend",
    "simplify code"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f develop.env nest start",
    "start:dev": "env-cmd -f develop.env nest start --watch",
    "start:debug": "env-cmd -f develop.env nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "yarn run generate:about && sh ./create-keys.sh && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.json -s",
    "build:docker": "docker-compose build",
    "start:docker": "docker-compose up -d",
    "stop:docker": "docker-compose down",
    "reset:docker": "yarn stop:docker && rimraf var && yarn start:docker",
    "logs:docker": "docker-compose logs -f",
    "generate:about": "ts-node ./generate-about.ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/swagger": "^4.5.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cron": "^1.8.2",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "mysql": "^2.18.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "unique-names-generator": "^4.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.1.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.9",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.3",
    "@types/lodash": "^4.14.150",
    "@types/moment-duration-format": "^2.2.2",
    "@types/mysql": "^2.15.10",
    "@types/node": "^14.0.14",
    "@types/randomstring": "^1.1.6",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "git-repo-info": "^2.1.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.1",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
