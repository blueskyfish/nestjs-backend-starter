{
  "name": "nestjs-backend-starter",
  "version": "0.0.1",
  "description": "A small nestjs backend with integration of mysql",
  "author": {
    "email": "blueskyfish@blueskyfish.de",
    "name": "Blueskyfish",
    "url": "https://blueskyfish.de"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/blueskyfish/nestjs-backend-starter.git"
  },
  "keywords": [
    "nestjs",
    "typescript",
    "express",
    "starter",
    "javascript",
    "docker",
    "mysql",
    "backend",
    "simplify code"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f develop.env nest start",
    "start:dev": "env-cmd -f develop.env nest start --watch",
    "start:debug": "env-cmd -f develop.env nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "sh ./create-keys.sh && jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.json -s",
    "build:docker": "docker-compose build",
    "start:docker": "docker-compose up"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/swagger": "^4.5.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cron": "^1.8.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql": "^2.18.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "unique-names-generator": "^4.2.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.1.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.9",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/mysql": "^2.15.10",
    "@types/node": "^13.13.0",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.5.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
